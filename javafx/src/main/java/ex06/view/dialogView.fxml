<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hbox" alignment="CENTER" prefHeight="50.0" prefWidth="560.0" spacing="5.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ex06.controller.DialogController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Button fx:id="btnOpen" mnemonicParsing="false" text="Open FileChooser" />
      <Button fx:id="btnSave" mnemonicParsing="false" text="Save FileChooser" />
      <Button fx:id="btnDir" mnemonicParsing="false" text="DirectoryChooser" />
      <Button fx:id="btnPopup" mnemonicParsing="false" text="Popup Dialog" />
      <Button fx:id="btnCustom" mnemonicParsing="false" text="CustomDialog" />
   </children>
</HBox>
